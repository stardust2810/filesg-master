  "scripts": {
    "start": "nx serve",
    "start:cron": "nx serve-cron",
    "build": "nx build",
    "test": "nx test",
    "prepare": "husky install",
    "docker:build": "nx docker:build",
    "docker:start": "docker-compose -f docker-compose.yml up -d --build",
    "docker:stop": "docker-compose -f docker-compose.yml down",
    "docker:start:test": "docker-compose -f docker-compose.test.yml --project-name filesg-test up -d --build",
    "docker:stop:test": "docker-compose -f docker-compose.test.yml --project-name filesg-test down",
    "localstack:start": "docker-compose -f ./localstack/docker-compose.localstack.yml up -d",
    "localstack:stop": "docker-compose -f ./localstack/docker-compose.localstack.yml down",
    "localstack:restart": "npm run localstack:stop && npm run localstack:start",
    "localstack:virus-scan:clean": "localstack/init-scripts/lambda/update-virus-scan-lambda.sh -t clean",
    "localstack:virus-scan:virus": "localstack/init-scripts/lambda/update-virus-scan-lambda.sh -t virus",
    "env:get:local": "nx env:get:local",
    "env:get:local:all": "nx run-many --target=env:get:local --all",
    "env:get:dev": "nx env:get:dev",
    "env:get:stg": "nx env:get:stg",
    "env:get:uat": "nx env:get:uat",
    "env:apply:local": "nx env:apply:local",
    "ddb:formsg:apply:local": "nx ddb:formsg:apply:local service-event-logs",
    "storybook:start": "nx storybook design-system",
    "storybook:build": "nx build-storybook design-system",
    "db-migration:seed": "nx db-migration:seed",
    "db-migration:run": "nx db-migration:run",
    "db-migration:revert": "nx db-migration-revert",
    "db-migration:create": "nx db-migration-create",
    "db-migration:generate": "nx db-migration-generate",
    "db-migration:show": "nx db-migration-show",
    "clean:nx-cache": "rimraf node_modules/.cache",
    "clean:dist": "rimraf dist apps/**/dist libs/**/dist",
    "clean:node-modules": "rimraf node_modules apps/**/node_modules libs/**/node_modules",
    "clean:db:setup:core": "npm run docker:stop && sleep 5 && npm run docker:start && sleep 10 && npm run db-migration:run service-core && sleep 5 && npx ts-node apps/service-core/sql/import.ts -e local",
    "pre-push:test": "nx affected:test --base=origin/$(git branch --show-current)",
    "find-unused:deps": "nx generate @trumbitta/nx-plugin-unused-deps:check --json --no-interactive",
    "find-unused:modules": "npx ts-unused-exports tsconfig.base.json --ignoreFiles=jest --excludePathsFromReport=migrations --excludeDeclarationFiles",
    "generate:email-template": "npx ts-node --project apps/service-core/src/common/email-template/scripts/tsconfig.json apps/service-core/src/common/email-template/scripts/create-mjml.ts -t",
    "generate:dev-portal-tech-doc": "npx ts-node scripts/tech-docs-generation/generate-tech-doc.ts"
  },
